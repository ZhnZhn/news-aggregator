{
  "name": "news-aggregator",
  "version": "0.4.0",
  "description": "Web app News Aggregator",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "css:sass": "gulp sass",
    "css:min": "gulp",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json ./app --no-open --mode static",
    "stats:help": "webpack-bundle-analyzer --help > out.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/news-aggregator.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^4.3.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "react-refresh": "^0.10.0",
    "sass": "^1.43.4",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "interactjs": "^1.3.4",
    "nanoid": "^3.1.31",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflux-core": "^1.0.0",
    "timeago.js": "^4.0.2"
  }
}
