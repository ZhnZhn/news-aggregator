{
  "name": "news-aggregator",
  "version": "0.4.0",
  "description": "Web app New Aggregator",
  "main": "index.js",
  "scripts": {
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "css:sass": "gulp sass",
    "css:min": "gulp",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static",
    "stats:help": "webpack-bundle-analyzer --help > out.txt",
    "start:emitter": "chokidar-socket-emitter",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/news-aggregator.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chokidar-socket-emitter": "^0.6.0",
    "del": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jspm": "^0.16.54",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "jspm": {
    "dependencies": {
      "interactjs": "npm:interactjs@1.3.4",
      "nanoid": "npm:nanoid@^2.1.6",
      "react": "npm:react@^16.11.0",
      "react-dom": "npm:react-dom@^16.11.0",
      "reflux": "npm:reflux@5.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@^1.1.0"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "interactjs": "^1.3.4",
    "nanoid": "^2.1.6",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "reflux": "^5.0.4"
  }
}
